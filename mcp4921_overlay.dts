/dts-v1/;
/plugin/;


/ {
        compatible = "brcm,bcm2835";

        fragment@0 {
                target = <&spi0>;
                frag1: __overlay__ {
                    #address-cells = <1>;
			        #size-cells = <0>;

                    pinctrl-names = "default";
                    pinctrl-0 = <&spi0_pins &spi0_cs_pins>;
                    cs-gpios = <&gpio 24 1>, <&gpio 7 1>, <&gpio 24 1>;
                
                    my_dev: mydev@2{
                        compatible = "mydev";
                        reg = <2>;
                        #address-cells = <1>;
                        #size-cells = <0>;
                        spi-max-frequency = <125000000>;
                    };
                };
        };
};